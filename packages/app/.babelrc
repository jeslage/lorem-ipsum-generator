{
  "presets": ["next/babel"],
  "plugins": [
    ["styled-components", { "ssr": true }],
    [
      "module-resolver",
      {
        "alias": {
          "@styles": "./styles",
          "@helper": "./helper",
          "@config": "./config",
          "@graphql": "./graphql",
          "@icons": "./components/icons",
          "@atoms": "./components/atoms",
          "@molecules": "./components/molecules",
          "@organisms": "./components/organisms",
          "@pages": "./components/pages",
          "@contexts": "./contexts"
        }
      }
    ]
  ]
}
